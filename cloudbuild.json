{
	"id": "hellonode_config",
	"steps": [
		{
			"name": "gcr.io/nick-cloudbuild/dedupe:latest",
			"args": [
				"python",
				"/dedupe.py",
				"gcr.io/$PROJECT_ID/hellonode:v3"
			],
			"id": "DEDUPE"
		},
		{
			"name": "gcr.io/cloud-builders/docker",
			"args": [
				"build",
				"--tag=gcr.io/$PROJECT_ID/hellonode:v3",
				"."
			],
			"id": "BUILD"
		}
	],
	"timeout": "60s",
	"images": [
		"gcr.io/$PROJECT_ID/hellonode:v3"
	]
}
