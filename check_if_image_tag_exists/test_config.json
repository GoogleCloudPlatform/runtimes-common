{
	"schemaVersion": "1.0.0",
	"commandTests": [
		{
			"name": "uname",
			"command": "uname",
			"flags": ["-s"],
			"expectedOutput": ["Linux\n"],
			"expectedError": [""]
		},
		{
			"name": "broken uname",
			"command": "uname",
			"flags": ["-s", "-asdfd"],
			"expectedOutput": [],
			"expectedError": [".*[invalid | unrecognized].*"]
		}
	],
	"fileExistenceTests": [
		{
			"name": "Root",
			"path": "/",
			"isDirectory": true,
			"shouldExist": true
		},{
			"name": "Fake file",
			"path": "/foo/bar",
			"isDirectory": false,
			"shouldExist": false
		}
	],
	"fileContentTests": [
		{
			"name": "main.py",
			"path": "/main.py",
			"expectedContents": ["#!/usr/bin/python.*"]
		}
	]
}
